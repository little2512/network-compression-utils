{
  "name": "network-compression-utils",
  "version": "1.0.0",
  "type": "module",
  "description": "A JavaScript library for network-aware data compression",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "network",
    "compression",
    "browser",
    "utility",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.22.0",
    "@babel/runtime-corejs3": "^7.28.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "core-js": "^3.47.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.0.0",
    "rollup": "^3.25.1"
  },
  "dependencies": {
    "lz-string": "^1.4.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
