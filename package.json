{
  "name": "network-compression-utils",
  "version": "1.0.2",
  "type": "module",
  "description": "A JavaScript library for intelligent network-aware data compression with real-time performance analysis",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:browser": "rollup -c rollup.browser.config.js",
    "build:simple": "rollup -c rollup.simple.config.js",
    "dev": "rollup -c -w",
    "test": "karma start karma.conf.cjs --single-run --browsers ChromeHeadless",
    "test:watch": "karma start karma.conf.cjs --auto-watch",
    "test:chrome": "karma start karma.conf.cjs --single-run --browsers ChromeHeadless",
    "test:firefox": "karma start karma.conf.cjs --single-run --browsers FirefoxHeadless",
    "test:headed": "karma start karma.conf.cjs --single-run --browsers Chrome",
    "test:debug": "karma start karma.conf.cjs --auto-watch --browsers Chrome",
    "test:coverage": "karma start karma.conf.cjs --single-run --browsers ChromeHeadless --reporters coverage,dots",
    "lint": "eslint src/**/*.js --ignore-pattern src/tests/**",
    "lint:fix": "eslint src/**/*.js --ignore-pattern src/tests/** --fix",
    "format": "prettier --write src/**/*.js",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "dry-run": "npm pack --dry-run",
    "version": "npm version",
    "postversion": "git push && git push --tags",
    "release": "node scripts/release.js",
    "release:patch": "npm run release patch",
    "release:minor": "npm run release minor",
    "release:major": "npm run release major",
    "release:publish": "npm run release publish",
    "release:dry-run": "npm run release dry-run"
  },
  "keywords": [
    "network",
    "compression",
    "browser",
    "utility",
    "performance",
    "javascript",
    "library",
    "lz-string",
    "network-aware",
    "adaptive",
    "optimization",
    "performance-analysis",
    "weak-network",
    "real-time",
    "transmission-optimization",
    "intelligent-compression"
  ],
  "author": "little2512",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/little2512/network-compression-utils.git"
  },
  "bugs": {
    "url": "https://github.com/little2512/network-compression-utils/issues"
  },
  "homepage": "https://github.com/little2512/network-compression-utils#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime-corejs3": "^7.28.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "core-js": "^3.47.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jasmine-core": "^5.13.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-jasmine": "^5.1.0",
    "prettier": "^3.0.0",
    "rollup": "^3.25.1"
  },
  "dependencies": {
    "lz-string": "^1.4.4",
    "qs": "^6.14.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
